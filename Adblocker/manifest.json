{
    "manifest_version": 3,
    "name": "AdBlocker",
    "description": "Adblock",
    "version": "1.1",
    "icons": {
   	 "32": "64.png",
   	 "48": "64.png",
   	 "128": "64.png"
	},
    "content_scripts": [{
        "js":["script.js"], 
       "run_at":"document_idle",
       "matches":["<all_urls>"]
       }],
       "permissions":[
        "tabs",
        "activeTab"
       ]
}