{
    "manifest_version": 2,
    "name": "AdBlocker By Yash",
    "version": "1.1.1",
    "description": "Yash Adblocker Tutorial",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "background": {
        "scripts": [
            "main.js"
        ]
    },
    "icons": {
        "16": "/icons/ad-blocker-16.png",
        "48": "/icons/ad-blocker-48.png",
        "128": "/icons/ad-blocker-128.png"
    }
}
